@prefix : <http://ld.sven.mol.it/ut-ldsw/holidays#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@base <http://www.utwente.nl/ldsw/energydata/holidays>.

<http://ld.sven.mol.it/ut-ldsw/holidays> rdf:type owl:Ontology.

# Object Properties
:fallsInAcademicYear rdf:type owl:ObjectProperty;
  rdfs:domain :Holiday;
  rdfs:range :AcademicYear.
:forRegion rdf:type owl:ObjectProperty;
  rdfs:domain :Holiday;
  rdfs:range :Region.

# Data properties
# TODO can we borrow this from an existing ontology?
:startDate rdf:type owl:DatatypeProperty;
  rdfs:range xsd:dateTime.
# TODO can we borrow this from an existing ontology?
:endDate rdf:type owl:DatatypeProperty;
  rdfs:range xsd:dateTime.
:isCompulsory rdf:type owl:DatatypeProperty;
  rdfs:range xsd:boolean.

# Classes
:AcademicYear rdf:type owl:Class.
:Holiday rdf:type owl:Class.
:Region rdf:type owl:Class.

# Static Individuals
:North rdf:type owl:NamedIndividual, :Region.
:Center rdf:type owl:NamedIndividual, :Region.
:South rdf:type owl:NamedIndividual, :Region.

# Dynamic Individuals
